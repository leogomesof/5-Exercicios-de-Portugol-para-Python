n1=int(input("Digite o primeiro número: "))
n2=int(input("Digite o segundo número: "))
n3=int(input("Digite o teceiro número: "))
maior=int
menor=int

if (n1>n2):
    maior=n1
    menor=n2
elif (n2>n1):
    maior=n2
    menor=n1

if (n3>maior):
    maior=n3
elif (n3<maior):
    menor=n3

print("\nO maior número é: {}\nO menor número é: {}".format(maior,menor))
